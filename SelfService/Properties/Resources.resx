  <data name="CoursesSQL" xml:space="preserve">
    <value>SELECT 
yn1.title AS 'registered',
yn2.title AS 'completed',
c.authorized_units,
c.course_name,
c.course_symbol,
c.gpa,
c.passed_units,
c.required_units,
c.passed_subjects,
c.required_subjects
FROM courses c
INNER JOIN yesno yn1 ON yn1.id = c.registered
INNER JOIN yesno yn2 ON yn2.id = c.completed
WHERE student_id = '{0}';</value>
  </data>
